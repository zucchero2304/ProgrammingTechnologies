<UserControl x:Class="Presentation.View.ClientDetailsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Presentation.View"
             xmlns:clients="clr-namespace:Presentation.ViewModel.Clients"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="CadetBlue">
    <UserControl.DataContext>
        <clients:ClientDetailsViewModel/>
    </UserControl.DataContext>

    <Grid>
            <Label Content="{Binding Id}"></Label>
    </Grid>
</UserControl>
